<div id="rename-box">
  <h1 i18n:translate="">
    Rename
    <span tal:replace="context/uniqueId"
      i18n:name="object-unique-id"/>
  </h1>
  <div class="warning"
    i18n:translate="rename-box-warning">
    Note: Renaming destroys links. 
  </div>

  <span class="nextUrl"
    tal:content="string:${context/@@absolute_url}/@@view.html" />

  <form id="rename.form" action="#"
    tal:attributes="action request/URL"
    tal:content="structure view/form">

  </form>

</div>  
