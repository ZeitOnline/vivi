<div class="object-widget"
  i18n:domain="zeit.cms"
  tal:attributes="id view/name">
  <input class="object-reference" type="text"
    tal:attributes="
    name view/name;
    value view/_getFormValue" />
  <input type="button" name="browseObjects" value="Lookup"
    i18n:attributes="value" />
</div>
<script tal:content="string:
  new ObjectReferenceWidget(
  '${view/name}', '${view/default_browsing_location/@@absolute_url}');
  ">
</script>
