<div class="objectsequencewidget"
  tal:attributes="id view/name">
  <tal:data tal:repeat="item view/_getFormValue">
      <input type="hidden" class="uniqueId"
        tal:attributes="
        name string:${view/name}.${repeat/item/index};
        id string:${view/name}.${repeat/item/index};
        value item/uniqueId"
        />
  </tal:data>
  <input tal:replace="structure view/marker" />

  <ul>
    <!-- data will be inserted here by javascript -->
  </ul>
  <div class="add_view"
       tal:condition="view/add_view">
    <a rel="show_add_view"
       tal:attributes="href view/add_view"
       i18n:translate="">
      Add new â€¦
    </a>
  </div>
  <div
    tal:condition="view/query_view"
    tal:content="structure view/query_view">
  </div>
</div>
<script tal:content="string:
  new zeit.cms.ObjectSequenceWidget('${view/name}', ${view/accept_classes});
">
</script>
