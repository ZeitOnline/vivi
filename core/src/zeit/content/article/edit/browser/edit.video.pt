<div i18n:domain="zeit.cms">

<div tal:attributes="
  cms:drop-url string:${context/@@absolute_url}/@@set_reference;
  class string:${view/css_class} video-image video_1 landing-zone visible">

  <span tal:condition="context/video"
       tal:content="structure context/video/@@object-details"/>
  <tal:noref condition="not:context/video">
    <tal:writable condition="view/writable">
      <span i18n:translate="">Drag content to embed.</span>
    </tal:writable>
    <tal:notwritable condition="not:view/writable">
      <span i18n:translate="">No content referenced.</span>
    </tal:notwritable>
  </tal:noref>

</div>

<div tal:attributes="
  cms:drop-url string:${context/@@absolute_url}/@@set_reference_2;
  class string:${view/css_class} video-image video_2 landing-zone visible">

  <span tal:condition="context/video_2"
       tal:content="structure context/video_2/@@object-details" />
  <tal:noref condition="not:context/video_2">
    <tal:writable condition="view/writable">
      <span i18n:translate="">Drag content to embed.</span>
    </tal:writable>
    <tal:notwritable condition="not:view/writable">
      <span i18n:translate="">No content referenced.</span>
    </tal:notwritable>
  </tal:noref>

</div>

<div style="clear: both"></div>

</div>
