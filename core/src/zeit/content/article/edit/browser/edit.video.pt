<div 
  tal:attributes="
  cms:drop-url string:${context/@@absolute_url}/@@set_reference;
  class view/css_class">

  <tal:reference condition="view/has_content">
    <div class="video-image video_1"
         tal:condition="context/video"
         tal:content="structure context/video/@@object-details"/>
    <div class="video-image video_2"
         tal:condition="context/video_2"
         tal:content="structure context/video_2/@@object-details" />
  </tal:reference>

  <tal:noref condition="not:context/video">
    <tal:writable condition="view/writable">
      <span i18n:translate="">Drag content to embed.</span>
    </tal:writable>
    <tal:notwritable condition="not:view/writable">
      <span i18n:translate="">No content referenced.</span>
    </tal:notwritable>
  </tal:noref>

</div>
