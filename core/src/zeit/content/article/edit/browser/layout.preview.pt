<div id="preview-tabs">
  <ul>
  </ul>
  <iframe
     id="preview-full"
     class="preview"
     tal:attributes="src string:${context/@@absolute_url}/@@show_preview">
  </iframe>
  <iframe
     id="preview-tablet"
     class="preview"
     tal:attributes="src string:${context/@@absolute_url}/@@show_preview?showtablett=on">
  </iframe>
</div>

<script language="javascript">
  (function($) {
    var tabs = $('#preview-tabs').tabs();
    tabs.tabs('add', '#preview-tablet', 'iPad');
    tabs.tabs('add', '#preview-full', 'Desktop');
  })(jQuery);
</script>
