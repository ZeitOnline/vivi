<div xmlns:cms="http://namespaces.gocept.com/zeit-cms"
      id="cp-content-inner"
      class="cp-content-inner article-editor-inner"
      i18n:domain="zeit.cms">
  <script
    type="text/javascript"
    tal:attributes="
    src string:${request/++resource++zeit.content.article.edit}/editor.js"></script>
  <script
    type="text/javascript"
    tal:attributes="
    src string:${request/++resource++zeit.content.article.edit}/ads.js"></script>
  <script
    type="text/javascript"
    tal:attributes="
    src string:${request/++resource++zeit.content.article.edit}/counter.js"></script>
