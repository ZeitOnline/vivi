<fieldset>
  <a id="checkin" cms:cp-module='follow-link'
          tal:attributes="href string:${context/@@absolute_url}/@@checkin"
          tal:condition="view/can_checkin"
          i18n:translate="">Checkin</a>
  <a id="checkin_correction" cms:cp-module='follow-link'
          tal:attributes="href string:${context/@@absolute_url}/@@checkin?semantic_change="
          tal:condition="view/can_checkin"
          i18n:translate="">Checkin (correction)</a>
  <div class="exit"
       tal:condition="view/can_checkin">
    <label i18n:translate="">...or cancel and</label>
    <a class="warning"
       id="delete_workingcopy"
       cms:cp-module='zeit.edit.LightBoxForm'
       tal:attributes="href string:${context/@@absolute_url}/@@delete.html"
       i18n:translate="">Delete from Workingcopy</a>
  </div>
</fieldset>
