<div id="tableFilter">Filter: <input name="tableFilter" type="text" value="" /></div>
<div id="tocListingFilter">
    <span
      tal:content="view/contenttype"
      i18n:translate="">
        Content-Typ
    </span>
    <select id="filter_content_typ">
    </select>
    <span
      tal:content="view/availability"
      i18n:translate="">
        Availability
    </span>
    <select id="filter_availibility">
    </select>
    <span
      tal:content="view/is_urgent"
      i18n:translate="">
        is urgent
    </span>
    <input type="checkbox" id="filter_is_urgent" />
    <span 
      tal:content="view/is_seo_optimized"
      i18n:translate="">
        is SEO optimized
    </span>
    <input type="checkbox" id="filter_is_optimized"/>
    <span
      tal:content="view/ressort"
      i18n:translate="">
        Ressort
    </span>
    <select id="filter_ressort" id="filter_ressort">
    </select>
    <span
      tal:content="view/teaser_image_available"
      i18n:translate="">
        Teaser image available
    </span>
    <input type="checkbox" id="filter_has_teaser_img"/>
    <span
      tal:content="view/supertitle_available"
      i18n:translate="">
        Supertitle available
    </span>
    <input type="checkbox" id="filter_has_supertitle"/>
    <span id="numberOfVisibleRows"></span>
    <strong
      tal:content="view/rows"
      i18n:translate="">
        rows
    </strong>
</div>
<form name="containerContentsForm" method="get"
      action="" id="folderActionForm">
  <div id="topcontent">
      <table
        tal:replace="structure view/contentTable">
        Table with contents...
      </table>

      <span class="Info"
        tal:condition="not:view/content"
        tal:content="view/no_content_message"
        i18n:translate="">
      There are no objects in this folder.
    </span>
  </div>
  <div id="bottomcontent"></div>
</form>