<div i18n:domain="zeit.cms"> <!-- lightbox -->
<h1 i18n:translate="">Publish</h1>

<tal:block condition="view/can_publish">
<ol>
<li id="publish.checkin">Check in</li>
<li id="publish.publish">Publish</li>
<li id="publish.checkout">Check out</li>
</ol>

<script type="text/javascript">
new zeit.content.cp.publish.Publisher();
</script>
</tal:block>

<div id="publish.errors">
  <tal:block tal:condition="not: view/can_publish">
    <p>Cannot publish since validation rules are violated.</p>
  </tal:block>
</div>
</div>

