<div i18n:domain="zeit.cms"> <!-- lightbox -->
<h1 i18n:translate="">Publish</h1>

<tal:block condition="view/can_publish">
<ol>
<li id="publish.checkin" i18n:translate="">Checking in</li>
<li id="publish.publish" i18n:translate="">Publishing</li>
<li id="publish.checkout" i18n:translate="">Checking out</li>
</ol>

<script type="text/javascript">
new zeit.content.cp.publish.Publisher();
</script>
</tal:block>

<div id="publish.errors">
  <tal:block tal:condition="not: view/can_publish">
    <p i18n:translate="">Cannot publish since validation rules are violated.</p>
  </tal:block>
</div>
</div>

