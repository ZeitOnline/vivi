<div class="block-title">RSS Feed</div>

<div class="block-item" tal:condition="not: context/feed/error"
  tal:content="string:${context/feed/title}">Titel</div>
<ul tal:condition="context/feed" class="feed-items">
  <li tal:repeat="entry context/feed/entries" tal:content="entry"/>
</ul>
<div class="block-item"
     tal:condition="context/feed/error"
     i18n:translate="">An error occurred.</div>
