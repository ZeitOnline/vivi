  zeit.cms.createDraggableContentObject('drag-foo');
  zeit.cms.createDraggableContentObject('drag-bar');
  zeit.edit.drop.registerContentHandler({
      accept: ['type-foo'],
      activated_by: 'foo-droppable'
  });
</script>

